{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022" /* Specify what module code is generated. */,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "jsx": "react-jsx",
    "declarationMap": true,
    "declaration": true,
    "lib": ["es2016", "esnext"],
    "rootDir": "./src",
    "outDir": "./dist",
    "types": ["./node_modules/@privy-io/server-auth/dist/index.d.ts"],
    "moduleResolution": "bundler",
    "resolvePackageJsonExports": true
  }
}
